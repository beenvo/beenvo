{
  "name": "beenvo",
  "version": "0.2.1",
  "description": "Beenvo let you loads environment variables from files (support many files extensions) for Node.js projects.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint . --ext .ts",
    "pretest": "rimraf dist",
    "test": "mocha --exit",
    "build": "tsc -d",
    "precoverage": "rimraf coverage .nyc_output",
    "coverage": "c8 npm run test",
    "postcoverage": "npm run build",
    "preci": "npm run lint",
    "ci": "npm run coverage",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run ci",
    "preversion": "npm run ci"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/3imed-jaberi/beenvo.git"
  },
  "keywords": [
    "beenvo",
    "dotenv",
    "env",
    ".env",
    "yaml",
    "yml",
    "xml",
    "properties",
    "toml",
    "json",
    "ini",
    "environment",
    "variables",
    "config",
    "settings"
  ],
  "license": "MIT",
  "author": {
    "name": "imed jaberi",
    "email": "imed-jaberi@outlook.com",
    "url": "https://www.3imed-jaberi.com"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "js-yaml": "^4.1.0",
    "properties": "^1.2.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.4.13",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "c8": "^7.8.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "mocha": "^9.0.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">= 12"
  },
  "bugs": {
    "url": "https://github.com/3imed-jaberi/beenvo/issues"
  },
  "homepage": "https://beenvo.github.io/beenvo/"
}
